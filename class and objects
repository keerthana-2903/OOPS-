class circle{
    public int radius;

    public int area(){			
        return radius*radius;
    }
    public double circumferece(){
        return 2*Math.PI*radius;
    }
}
public class Main {
    public static void main(String[] args) {

        circle c=new circle();
        c.radius=5;
        System.out.println(c.area());
        System.out.println(c.circumferece());
        }
    }
